package phase1_programs;

public class Constructor {
	public Constructor(){
		System.out.println("This is the default constructor");
		
	}
	public Constructor(int a){
		System.out.println("This is a parameterized constructor");
		System.out.println("The value of  variabe a is "+a);
		
	}
	public Constructor(int a ,int b){
		System.out.println("This is a parameterized constructor");
		System.out.println("The value of variable a \n"+a );
		System.out.println("The value of variable b \n"+b );
		
	}


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Constructor c1 = new Constructor();
		Constructor c2 = new Constructor(10);
		Constructor c3 = new Constructor(11,22);



	}

}
